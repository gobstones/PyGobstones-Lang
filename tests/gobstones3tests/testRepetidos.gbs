program {
  return(repetidos(3, [3,1,4,1,-8,7,7,7]))
}

// rep
function repeticionesEn(ls) {
  res:=[]
  foreach i in ls {
    res:=res++[repetidos(i, ls)]
  }
  return(res)
}


function repetidos(e, ls) {
  cont:=0

  foreach i in ls {
    if(e == i) {
      cont:=cont+1
    }
  }
  return([e, cont])
}
